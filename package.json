  "main": "index.js",
  "scripts": {
    "start": "nodemon  ./server/app --watch ./server --exec babel-node",
    "migrate": "sequelize db:migrate && npm run seed",
    "migrate:prod": "cross-env NODE_ENV=production sequelize db:migrate && npm run seed:prod",
    "migrate:test": "cross-env NODE_ENV=test sequelize db:migrate",
    "undo:migrate": "sequelize db:migrate:undo:all",
    "undo:migrate:prod": "cross-env NODE_ENV=production sequelize db:migrate:undo:all",
    "undo:migrate:test": "cross-env NODE_ENV=test sequelize db:migrate:undo:all",
    "run:test": "npm run undo:migrate:test && npm run migrate:test  && npm run test:dev",
    "seed": "sequelize db:seed:all",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "test:dev": "cross-env NODE_ENV=test mocha  --compilers js:babel-register --timeout 500000",
    "test": "npm run run:test",
    "prebuild": "rm -rf dist/*",
    "build:prod": "npm run prebuild && babel -d ./dist/server ./server -s"
  },
